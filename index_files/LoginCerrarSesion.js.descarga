var Hash=window.localStorage.getItem('Hash')
if(Token!=null&&Usuario!=null&&Hash!=null){}});function SalirSesionLocal(Token,Usuario,Hash){const formData=new FormData();formData.append("token",Token);formData.append("usuario",Usuario);formData.append("encripta",Hash);$.ajax({type:"POST",url:"/Cuenta/SalirSesionStorage",contentType:false,data:formData,dataType:"json",processData:false,success:function(response){if(response){window.localStorage.removeItem('Token');window.localStorage.removeItem('UsuEn');window.localStorage.removeItem('Hash');}},failure:function(response){},error:function(response){}});}
