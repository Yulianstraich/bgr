function VerOcultarMontos(){if($('.input-monto').attr('type')=="password"){$('.input-monto').attr('type','text');$("#biMonto").removeClass("bi-eye-fill").addClass("bi-eye-slash-fill");$("#mostrar").attr("src","/img/brand/ojo_ocultar.svg");$("#mostrar").attr("data-bs-original-title","Ocultar Montos");OcultaMontosControlador("1");}
else{$('.input-monto').attr('type','password')
$("#biMonto").removeClass("bi-eye-slash-fill").addClass("bi-eye-fill");$("#mostrar").attr("src","/img/brand/ojo_sin_ocultar.svg");$("#mostrar").attr("data-bs-original-title","Mostrar Montos");OcultaMontosControlador("2");}
var mostrar=document.getElementById('mostrar');var tooltip=bootstrap.Tooltip.getInstance(mostrar);if(tooltip){tooltip.dispose();new bootstrap.Tooltip(mostrar);}}
function OcultaMontosControlador(param){const formData=new FormData();formData.append("parametro",param);$.ajax({type:"POST",url:"/MetodosComunes/OcultarMontos",contentType:false,dataType:"json",data:formData,processData:false,success:function(response){console.log('OK');},failure:function(response){console.log('error');},error:function(response){console.log('error');}});}
let eventVisualizarClave=(input,icono)=>{$(input).attr("type","text");$(icono).attr("src","/img/brand/ojo_azul_mostrar.svg");};let eventOcultarClave=(input,icono)=>{$(input).attr("type","password");$(icono).attr("src","/img/brand/ojo_azul_ocultar.svg");};